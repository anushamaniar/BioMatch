<?xml version="1.0" encoding="utf-8"?>

<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/container_login_home"
    android:layout_width="match_parent"
    android:layout_height="812dp"
    android:background="@drawable/container_login_home">

    <FrameLayout
        android:id="@+id/container_group"
        android:layout_width="41dp"
        android:layout_height="41dp"
        android:layout_marginLeft="323dp"
        android:layout_marginTop="37dp"
        android:layout_gravity="top|left">

        <FrameLayout
            android:id="@+id/image_back"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/image_back">

            <ImageView
                android:layout_width="41dp"
                android:layout_height="41dp"
                android:background="@drawable/drawable_shape"
                android:scaleType="centerInside" />
        </FrameLayout>

        <ImageView
            android:id="@+id/image_settings"
            android:layout_width="26dp"
            android:layout_height="28dp"
            android:layout_gravity="top"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="7dp"
            android:layout_marginRight="7dp"
            android:scaleType="centerInside"
            android:src="@drawable/image_settings" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/container_back_to_login_button1"
        android:layout_width="331dp"
        android:layout_height="56dp"
        android:layout_marginLeft="22dp"
        android:layout_marginTop="378dp"
        android:layout_gravity="top|left"
        android:background="@drawable/container_back_to_login_button1">

        <ImageView
            android:id="@+id/image_rectangle1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/image_rectangle1"
            android:scaleType="centerCrop" />

        <TextView
            android:id="@+id/text_update"
            android:layout_width="wrap_content"
            android:layout_height="32dp"
            android:layout_marginLeft="138dp"
            android:layout_marginRight="138dp"
            android:layout_marginTop="12dp"
            android:layout_marginBottom="12dp"
            android:gravity="center_horizontal|top"
            android:includeFontPadding="false"
            android:lineHeight="31.68dp"
            android:text="Update"
            android:textColor="#FFFFFFFF"
            android:textSize="15dp" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/container_back_to_login_button"
        android:layout_width="331dp"
        android:layout_height="56dp"
        android:layout_marginLeft="22dp"
        android:layout_marginTop="468dp"
        android:layout_gravity="top|left"
        android:background="@drawable/container_back_to_login_button">

        <ImageView
            android:id="@+id/image_rectangle"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/image_rectangle"
            android:scaleType="centerCrop" />

        <TextView
            android:id="@+id/text_authenticate"
            android:layout_width="wrap_content"
            android:layout_height="32dp"
            android:layout_marginLeft="116dp"
            android:layout_marginRight="116dp"
            android:layout_marginTop="12dp"
            android:layout_marginBottom="12dp"
            android:gravity="center_horizontal|top"
            android:includeFontPadding="false"
            android:lineHeight="31.68dp"
            android:text="Authenticate"
            android:textColor="#FFFFFFFF"
            android:textSize="15dp" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/btn_back_to_login"
        android:layout_width="331dp"
        android:layout_height="56dp"
        android:layout_marginLeft="22dp"
        android:layout_marginTop="288dp"
        android:layout_gravity="top|left"
        android:background="@drawable/btn_back_to_login">

        <ImageView
            android:id="@+id/img_rectangle"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/img_rectangle"
            android:scaleType="centerCrop" />

        <TextView
            android:id="@+id/txt_enroll"
            android:layout_width="wrap_content"
            android:layout_height="32dp"
            android:layout_marginLeft="143dp"
            android:layout_marginTop="12dp"
            android:layout_marginBottom="12dp"
            android:layout_gravity="left"
            android:gravity="center_horizontal|top"
            android:includeFontPadding="false"
            android:lineHeight="31.68dp"
            android:text="Enroll"
            android:textColor="#FFFFFFFF"
            android:textSize="15dp" />
    </FrameLayout>

    <TextView
        android:id="@+id/text_welcome_back"
        android:layout_width="wrap_content"
        android:layout_height="55dp"
        android:layout_gravity="top|left"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="184dp"
        android:gravity="center_horizontal|top"
        android:includeFontPadding="false"
        android:lineHeight="54.91dp"
        android:text="Welcome Back!"
        android:textColor="#FF1E232C"
        android:textSize="26dp" />

    <FrameLayout
        android:id="@+id/container_back"
        android:layout_width="41dp"
        android:layout_height="41dp"
        android:layout_marginTop="29dp"
        android:layout_gravity="top">

        <ImageView
            android:id="@+id/image_rectangle2"
            android:layout_width="41dp"
            android:layout_height="41dp"
            android:background="@drawable/image_rectangle2"
            android:scaleType="centerInside" />

        <ImageView
            android:id="@+id/image_back_arrow"
            android:layout_width="19dp"
            android:layout_height="19dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="12dp"
            android:layout_marginTop="11dp"
            android:layout_marginBottom="11dp"
            android:scaleType="centerInside"
            android:src="@drawable/image_back_arrow" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/container_status_bar"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:layout_gravity="top">

        <FrameLayout
            android:id="@+id/container_left_side"
            android:layout_width="54dp"
            android:layout_height="21dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="12dp"
            android:layout_gravity="top|left"
            android:background="@drawable/container_left_side"/>

    </FrameLayout>
</FrameLayout>
